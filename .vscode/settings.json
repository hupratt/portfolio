{
  "python.pythonPath": "env/bin/python3.10",
  "editor.formatOnSave": false,
  "python.linting.pycodestyleEnabled": false,
  "python.linting.pylintPath": "pylint",
  "python.linting.pylintArgs": ["--load-plugins", "pylint_django"],
