  "python.venvPath": "${workspaceFolder}/env/bin/python3.10",
  "python.linting.pycodestyleArgs": ["--ignore=E501"],
  "files.exclude": {
    "**/*.pyc": true
  },
  "python.formatting.provider": "black",
  "python.linting.mypyEnabled": true,
  "python.linting.enabled": true
